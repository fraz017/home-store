<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Rooms</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_rooms.each do |api_v1_room| %>
      <tr>
        <td><%= link_to 'Show', api_v1_room %></td>
        <td><%= link_to 'Edit', edit_api_v1_room_path(api_v1_room) %></td>
        <td><%= link_to 'Destroy', api_v1_room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room', new_api_v1_room_path %>
